<h1>Listing firms</h1>

<table>
  <thead>
    <tr>
      <th>Firm</th>
      <th>Type</th>
      <th>Sector</th>
      <th>Specialties</th>
      <th>Vintage</th>
      <th>Location</th>
      <th>Size</th>
      <th>Projects</th>
      <th>Top 10 products</th>
      <th>Accolades</th>
      <th>State licensure</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Website</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @firms.each do |firm| %>
      <tr>
        <td><%= firm.firm_id %></td>
        <td><%= firm.type %></td>
        <td><%= firm.sector %></td>
        <td><%= firm.specialties %></td>
        <td><%= firm.vintage %></td>
        <td><%= firm.location %></td>
        <td><%= firm.size %></td>
        <td><%= firm.projects %></td>
        <td><%= firm.top_10_products %></td>
        <td><%= firm.accolades %></td>
        <td><%= firm.state_licensure %></td>
        <td><%= firm.name %></td>
        <td><%= firm.email %></td>
        <td><%= firm.phone %></td>
        <td><%= firm.website %></td>
        <td><%= link_to 'Show', firm %></td>
        <td><%= link_to 'Edit', edit_firm_path(firm) %></td>
        <td><%= link_to 'Destroy', firm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Firm', new_firm_path %>
